    <refnamediv>
        <refname>mdaq_dio_set</refname>
        <refpurpose>
		
		Sets MicroDAQ DIO state
		
		</refpurpose>
    </refnamediv>
    <refsection>
        <title>Block Screenshot</title>
        <para>
            <inlinemediaobject>
                <imageobject>
