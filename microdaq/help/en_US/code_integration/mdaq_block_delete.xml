xml:id="mdaq_block_delete">
  <refnamediv>
    <refname>mdaq_block_delete</refname>
    <refpurpose>Deletes MicroDAQ user block</refpurpose>
  </refnamediv>
  <refsynopsisdiv>
	<title>Description</title>
	<para>This function deletes all files (C source, macros, images) related with given block name.</para>
	<para>
	
	</para>
    <title>Calling Sequence</title>
    <synopsis>
      mdaq_block_delete(block_name);
    </synopsis>
  </refsynopsisdiv>
  <refsection id="Parameters___FUNC_NAME__">
    <title>Arguments</title>
    <itemizedlist>
      <listitem>
	    <para><emphasis role="bold">block_name : </emphasis> Valid user block name.</para>
      </listitem>	  
    </itemizedlist>
  </refsection>
  
    	<refsection>
        <title>Examples</title>
        <programlisting role="example"><![CDATA[
mdaq_block_delete("My_step");
 ]]></programlisting>
    </refsection>
	   
  <refsection id="SeeAlso___FUNC_NAME__">
